{
  "plugins": [
    "remark-preset-lint-recommended",
    "remark-preset-lint-consistent",
    ["remark-lint-list-item-indent", "space"],
    ["remark-lint-ordered-list-marker-value", "ordered"],
    ["remark-lint-unordered-list-marker-style", "-"],
    ["remark-lint-no-dead-urls", true],
    ["remark-lint-maximum-line-length", 100],
    ["remark-lint-no-duplicate-headings", true],
    ["remark-lint-no-multiple-toplevel-headings", true],
    ["remark-lint-heading-style", "atx"],
    ["remark-lint-no-emphasis-as-heading", true],
    ["remark-lint-strong-marker", "*"],
    ["remark-lint-emphasis-marker", "_"],
    ["remark-lint-fenced-code-flag", true],
    ["remark-lint-code-block-style", "fenced"]
  ]
}
